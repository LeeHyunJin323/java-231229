package a_basic;


//주석 처리 된 부분
// : 코드에 대한 설명과 메모를 담당하는 기능 -> 주석 , 단축키 컨트롤 + /
/*이것은
 * 여러줄 주석
 * 입니다.
 */

//자바 프로그래밍 기본 구조
//클래스 > 매서드 > 실행문 


//클래스의 시작 
public class A_Hellojava {

	// 메서드의 시작 
	
	public static void main(String[] args) {
		//컴퓨터에게 전달할 명령 (실행문)을 작성
		// - 자바의 출력
		System.out.println("Hello JAVA!!"); // ln을 넣으면 \n 
		System.out.println("Hello Coding!");	
		//자바 자동 완성은 컨트롤 + 스페이스를 통해서 가능
	    //syso -> 컨트롤 + 스페이스바 하면 System.out.println 자동 작성
		
		
	
		
	}
	
}
